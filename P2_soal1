#include <iostream>
#include <string>

using namespace std;

int main()
{ 
	string nama;
	nama = "Abdullah";

	int umur;
	umur = 64;

	float tinggi;
	tinggi = 173.64;

	string hobby;
	hobby = "main bola, nulis, ngoding";

	cout <<"nama		= "<<nama<<'\n';
	cout <<"umur		= "<<umur<<'\n';
	cout <<"tinggi		= "<<tinggi<<'\n';
	cout <<"hobby		= "<<hobby<<'\n';
	return 0;
}



